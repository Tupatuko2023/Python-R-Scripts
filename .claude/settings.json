{
  "permissions": {
    "defaultMode": "default",
    "ask": [
      "Bash(git push:*)",
      "Bash(git reset --hard:*)",
      "Bash(git clean:*)",
      "Bash(rm -rf:*)",
      "Bash(rm -r:*)",
      "Bash(mv *:**/.*)",
      "Bash(curl:*)",
      "Bash(wget:*)",
      "Bash(docker:*)",
      "Bash(docker-compose:*)"
    ],
    "deny": [
      "Read(./.env)",
      "Read(./.env.*)",
      "Read(./secrets/**)",
      "Read(~/.aws/**)",
      "Read(~/.ssh/**)",
      "Read(**/id_rsa)",
      "Read(**/id_rsa.*)",
      "Read(**/.env)",
      "Read(**/.env.*)"
    ]
  }
}
